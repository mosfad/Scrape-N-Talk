<nav>
    <span>
        <h2>Scrape-N-Talk</h2>
    </span>
    <a href="index.handlebars">Home</a>
    <a href="savedArticles.handlebars">Saved Articles</a>
    <button id="get-articles">SCRAPE NEW ARTICLES</button>
</nav>

<div id="img-container">
    <img src="images/readingNews.jpg" alt="Background image">
    <div class="img-text">
        <h1>Saved Articles</h1>
        <h3>Your Saved Articles</h3>
    </div>
</div>

<div class="row">
    <div class="col s12 m12">

        <ul class="collection with-header saved-articles">



            {{#each Articles}}
            <div class="col s12 m12">
                <div class="card">
                    <div class="card-content black-text">
                        <li class="collection-header brown darken-1 single-article">
                            <h4>{{this.title}}</h4><span><a class="waves-effect waves-light btn">ARTICLE
                                    NOTES</a></span>
                            <span><a class="waves-effect waves-light btn red">DELETE FROM SAVED</a></span>
                        </li>
                        <li class="collection-item single-article"><a href="{{this.link}}">{{this.link}}</a></li>
                        <li class="collection-item single-article">{{this.summary}}</li>
                    </div>
                </div>

            </div>


            {{/each}}
        </ul>


    </div>
</div>